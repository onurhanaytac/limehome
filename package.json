{
  "name": "limehome-client",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest --no-cache",
    "generate": "node ./util/create-component",
    "prebuild": "rimraf dist && npm run lint && npm run format",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x} --fix",
    "svgr": "node util/svgr-suffix.js && rimraf src/components/Icons && svgr --ref --ext tsx --template util/svgr-template.js -d src/components/Icons/ src/assets/icons && npm run format && npm run lint"
  },
  "dependencies": {
    "@react-google-maps/api": "^2.8.1",
    "clsx": "1.0.3",
    "formik": "^2.2.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-jss": "^10.9.0",
    "react-router-dom": "^6.3.0",
    "react-test-renderer": "^18.0.0",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@honkhonk/vite-plugin-svgr": "^1.1.0",
    "@svgr/cli": "^6.2.1",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^12.1.5",
    "@types/jest": "^26.0.15",
    "@types/joi": "^17.2.3",
    "@types/node": "^15.14.1",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-jss": "^10.0.0",
    "@types/supercluster": "^7.1.0",
    "@types/uuid": "^8.3.4",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@vitejs/plugin-react": "^1.0.7",
    "colors": "^1.4.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.6.3",
    "vite": "^2.9.0",
    "vite-plugin-svgr": "^1.1.0"
  }
}
